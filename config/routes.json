{
  "": {
    "controller": "Customer",
    "method": "startPage"
  },
  "customers": {
    "controller": "Customer",
    "method": "getAll"
  },
  "cars": {
    "controller": "Car",
    "method": "getAll"
  },
  "checkout": {
    "controller": "Car",
    "method": "checkOutCar"
  },
  "checkin": {
    "controller": "Car",
    "method": "checkInCar"
  },
  "history": {
    "controller": "Rental",
    "method": "getHistory"
  },
  "sales/:id": {
    "controller": "Sales",
    "method": "get",
    "login": true,
    "params": {
      "id": "number"
    }
  },
  "my-books": {
    "controller": "Book",
    "method": "getByUser",
    "login": true
  },
  "book/:id/buy": {
    "controller": "Sales",
    "method": "add",
    "login": true,
    "params": {
      "id": "number"
    }
  },
  "book/:id/borrow": {
    "controller": "Book",
    "method": "borrow",
    "login": true,
    "params": {
      "id": "number"
    }
  },
  "book/:id/return": {
    "controller": "Book",
    "method": "returnBook",
    "login": true,
    "params": {
      "id": "number"
    }
  }
}
